{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "selector scheme",
  "type": "array",
  "required": [ "host", "selectors", "startIndex", "isNecessaryFileNumber" ],
  "additionalProperties": false,
  "properties": {
    "host": {
      "type": "string"
    },
    "selectors": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "startIndex": {
      "type": "number"
    },
    "isNecessaryFileNumber": {
      "type": "boolean"
    }
  }
}